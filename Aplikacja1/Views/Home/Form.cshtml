@model Aplikacja1.Models.FormModel
@{ 
    ViewData["Title"] = "Form";
}

<div class="left">
    <h1 class="display-4">Imię i liczba powtórzeń</h1>
    <h4>Imię: @Html.DisplayFor(model => model.Name)</h4>
    <h4>Liczba Powtórzeń: @Html.DisplayFor(model => model.NumOfReplications)</h4>
    @for(int i=0;i<Model.NumOfReplications;i++)
            {
                <h6>@Html.DisplayFor(model => model.Name)</h6>
            }
</div>
<a href="~/index.html">Powrót</a>